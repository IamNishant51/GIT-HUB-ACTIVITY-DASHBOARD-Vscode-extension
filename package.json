{
  "name": "github-activity-dashboard",
  "displayName": "GitHub Activity Dashboard",
  "description": "A dashboard to track your GitHub activity.",
  "version": "1.6.0",
  "icon": "logo.png",
  "publisher": "NishantUnavane",
  "engines": {
    "vscode": "^1.80.0"
  },
  "galleryBanner": {
    "color": "#0d1117",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
      "keywords": [
    "GitHub",
    "Dashboard",
    "Activity",
    "Repositories",
    "Profile",
    "History",
    "Starred Repos",
    "Notifications",
    "Profile Repositories"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "github-dashboard-container",
          "title": "GitHub Dashboard",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "github-dashboard-container": [
        {
          "id": "github-activity-dashboard",
          "name": "Activity",
          "icon": "resources/icon.svg"
        },
        {
          "id": "github-repositories",
          "name": "Repositories",
          "icon": "resources/icon.svg"
        },
        {
          "id": "github-profile",
          "name": "Profile",
          "icon": "resources/icon.svg"
        },
        {
          "id": "github-history",
          "name": "History",
          "icon": "resources/icon.svg"
        },
        {
          "id": "github-stars",
          "name": "Starred Repos",
          "icon": "resources/icon.svg"
        },
        {
          "id": "github-notifications",
          "name": "Notifications",
          "icon": "resources/icon.svg"
        },
        {
          "id": "github-profile-repos",
          "name": "Profile Repositories",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "github-activity-dashboard.refresh",
        "title": "Refresh GitHub Activity",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "github-activity-dashboard.createIssue",
        "title": "Create New Issue",
        "icon": "$(issues)"
      },
      {
        "command": "github-activity-dashboard.searchRepos",
        "title": "Search GitHub Repositories",
        "icon": "$(search)"
      },
      {
        "command": "github-activity-dashboard.openRepo",
        "title": "Open Repository in VS Code",
        "icon": "$(repo)"
      },
      {
        "command": "github-activity-dashboard.openProfile",
        "title": "Open Profile in VS Code",
        "icon": "$(account)"
      },
      {
        "command": "github-activity-dashboard.openStarredFile",
        "title": "Open Starred File",
        "icon": "$(file)"
      },
      {
        "command": "github-activity-dashboard.checkoutCommit",
        "title": "Checkout Commit",
        "icon": "$(git-commit)"
      },
      {
        "command": "github-activity-dashboard.openFile",
        "title": "Open File",
        "icon": "$(file)"
      },
      {
        "command": "github-activity-dashboard.testExpandRepo",
        "title": "Test Expand Repository (Debug)",
        "icon": "$(debug)"
      },
      {
        "command": "github-activity-dashboard.createBranch",
        "title": "Create Branch",
        "icon": "$(git-branch)"
      },
      {
        "command": "github-activity-dashboard.switchBranch",
        "title": "Switch Branch",
        "icon": "$(git-branch)"
      },
      {
        "command": "github-activity-dashboard.viewRepositoryStats",
        "title": "View Repository Statistics",
        "icon": "$(graph)"
      },
      {
        "command": "github-activity-dashboard.searchCode",
        "title": "Search Code in Repository",
        "icon": "$(search)"
      },
      {
        "command": "github-activity-dashboard.createRepo",
        "title": "Create New Repository",
        "icon": "$(add)"
      },
      {
        "command": "github-activity-dashboard.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "github-activity-dashboard.createRepo",
          "when": "view == github-profile-repos",
          "group": "navigation",
          "icon": "$(add)"
        },
        {
          "command": "github-activity-dashboard.refresh",
          "when": "view == github-profile-repos",
          "group": "navigation",
          "icon": "$(refresh)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IamNishant51/github-activity-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/IamNishant51/github-activity-dashboard/issues"
  },
  "homepage": "https://github.com/IamNishant51/github-activity-dashboard",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.44.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.2",
    "simple-git": "^3.28.0"
  }
}